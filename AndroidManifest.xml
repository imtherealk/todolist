<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.realk.todolist" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.TodoListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AddItemActivity"
            android:label="AddItemActivity" >
        </activity>
        <activity
            android:name=".activity.DetailViewActivity"
            android:label="DetailViewActivity" >
        </activity>
        <activity
            android:name=".activity.ModifyItemActivity"
            android:label="ModifyItemActivity" >
        </activity>
    </application>

</manifest>
